<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hellow">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.HistoryActivity"></activity>
        <activity android:name=".activities.MatrixSelectionActivity" />
        <activity android:name=".activities.CalculationActivity" />
        <activity android:name=".activities.ViewMatricesActivity" />
        <activity android:name=".activities.MatrixCreationActivity" />
        <activity android:name=".activities.DisplayMatrixActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>